{
  "name": "datecounter",
  "version": "1.0.0",
  "description": "Count the number of days that have passed in this year",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Date-Counter --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Date Counter\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/.png --prune=true --out=release-builds"
  },
  "author": "Aritro Sinha",
  "license": "MIT",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.2.0"
  },
  "dependencies": {}
}
